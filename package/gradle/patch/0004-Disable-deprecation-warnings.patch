+++ b/build-logic/jvm/src/main/kotlin/gradlebuild.strict-compile.gradle.kts
@@ -18,7 +18,7 @@ import gradlebuild.jvm.extension.StrictCompileExtension
 
 extensions.create<StrictCompileExtension>("strictCompile")
 
-val strictCompilerArgs = listOf("-Werror", "-Xlint:all", "-Xlint:-options", "-Xlint:-serial", "-Xlint:-classfile", "-Xlint:-try")
+val strictCompilerArgs = listOf("-Werror", "-Xlint:all", "-Xlint:-options", "-Xlint:-serial", "-Xlint:-classfile", "-Xlint:-try", "-Xlint:-deprecation")
 
 tasks.withType<JavaCompile>().configureEach {
     // Generated classes may not adhere to the strict no-warning policy that we apply to handwritten code
